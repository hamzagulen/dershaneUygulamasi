@model Kullanici


<div class="row justify-content-center">
    <div class="col-md-6">
        <h2 class="mb-4">Profil Bilgileri</h2>
        <hr class="my-2">

        <dl class="row">
            <dt class="col-sm-3">Ad</dt>
            <dd class="col-sm-9">@Model.Ad</dd>

            <dt class="col-sm-3">Soyad</dt>
            <dd class="col-sm-9">@Model.Soyad</dd>

            <dt class="col-sm-3">E-posta</dt>
            <dd class="col-sm-9">@Model.Email</dd>

            <dt class="col-sm-3">Telefon</dt>
            <dd class="col-sm-9">@Model.Telefon</dd>

            <dt class="col-sm-3">TC Kimlik</dt>
            <dd class="col-sm-9">@Model.TcKimlik</dd>
        </dl>
    </div>
</div>
